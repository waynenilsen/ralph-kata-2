model Session {
  id           String   @id @default(cuid())
  user         User     @relation(fields: [userId], references: [id])
  userId       String
  tenantId     String
  expiresAt    DateTime
  createdAt    DateTime @default(now())
  userAgent    String?
  ipAddress    String?
  lastActiveAt DateTime @default(now())

  @@index([userId])
  @@index([tenantId])
}
